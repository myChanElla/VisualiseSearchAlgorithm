package uielements;

import java.awt.Color;

public class CustomiseUIColours {

	public static Color settAsBackGroundColour()
	{
		Color backgroundcolor = new Color(50,62,79);
		return backgroundcolor;
		
	}
	public static Color settAsPrimaryColour()
	{
		Color primarycolor = new Color(166,179,198);
		return primarycolor;
	
	}
	public static Color setAsAccentColour()
	{
		Color accentcolor = new Color(0, 162, 232);
		return accentcolor; 
		
	}
	public static Color settAsTextColour()
	{
		Color textcolor = Color.decode("#FAFAFA");
		return textcolor;
	}
	
}
