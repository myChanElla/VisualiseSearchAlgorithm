import java.awt.Graphics;

import javax.swing.JPanel;

@SuppressWarnings("serial")
public class GraphVisualiser extends JPanel {

	GraphComponent comp;
	/**
	 * Create the panel.
	 */
	public GraphVisualiser(GraphComponent comp) {
		this.comp = comp;
	}
	public void paintComponent(Graphics g)
	{
		comp.paint(g);
	}
	

}
